@model PTLab2_Final.Models.Electronic
 
<h2>Добавление пользователя</h2>
<form asp-action="create" asp-controller="home">
    <p>
        <label asp-for="Name">Имя</label><br />
        <input type="text" asp-for="Name" />
    </p>
    <p>
        <label asp-for="Category">Категория</label><br />
        <input type="text" asp-for="Category" />
    </p>
    <p>
        <label asp-for="Price">Цена</label><br />
        <input type="number" asp-for="Price" />
    </p>
    <p>
        <label asp-for="ForSale">Всего товара</label><br />
        <input type="number" asp-for="ForSale" />
    </p>
    <p>
        <label asp-for="Sold">Продано</label><br />
        <input type="number" asp-for="Sold" />
    </p>
    <p>
        <label asp-for="Counter">Счетчик</label><br />
        <input type="number" asp-for="Counter" />
    </p>
    <p>
        <input type="submit" value="Отправить" />
    </p>
</form>